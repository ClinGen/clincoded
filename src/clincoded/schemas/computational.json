{
    "title": "Computational",
    "id": "/profiles/computational.json",
    "description": "Schema for storing computational data for Variant Curation",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [],
    "identifyingProperties": ["uuid"],
    "additionalProperties": false,
    "mixinProperties": [
        { "$ref": "mixins.json#/schema_version" },
        { "$ref": "mixins.json#/uuid" },
        { "$ref": "mixins.json#/standard_status" },
        { "$ref": "mixins.json#/submitted" }
    ],
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "variant": {
            "title": "Variant",
            "type": "string",
            "linkTo": "variant"
        },
        "Missense": {
            "title": "Missense",
            "description": "Object Missense",
            "type": "object",
            "properties": {
                "computationalTools": {
                    "title": "Computational Tools",
                    "type": "object",
                    "properties": {
                        "overAllValue": {
                            "title": "Overall",
                            "description": "Overall value of all computational tools",
                            "type": "string"
                        },
                        "proteinPredictors": {
                            "title": "Protein Predictors",
                            "description": "Value of protein predictors",
                            "type": "string"
                        },
                        "conservationAnalysis": {
                            "title": "Conservation Analysis",
                            "description": "Value of conservation Analysis",
                            "type": "string"
                        },
                        "splicingPredictors": {
                            "title": "Splicing Predictors",
                            "description": "Value of splicing predictors",
                            "type": "string"
                        }
                    }
                },
                "otherVariantsInCodon": {
                    "title": "Other Variants in Codon",
                    "description": "List of variants.",
                    "type": "array",
                    "items": {
                        "title": "Variant",
                        "type": "object",
                        "properties": {
                            "source": {
                                "title": "source",
                                "description": "External source of the variant",
                                "type": "string"
                            },
                            "sourceId": {
                                "title": "Source ID",
                                "description": "Primary key of the variant in external source",
                                "type": "string"
                            },
                            "clinivalType": {
                                "title": "Clinical Type",
                                "description": "Varaint interpretation on pathogenicity",
                                "type": "string"
                            },
                            "protein": {
                                "title": "Protein Presentation",
                                "type": "string"
                            },
                            "cDNA": {
                                "title": "cDNA Presentation",
                                "type": "string"
                            }
                        }
                    }
                },
                "differentChange": {
                    "title": "Different Change",
                    "description": "Different pathogenic missense change at residue?",
                    "type": "boolean"
                },
                "sameAAChange": {
                    "title": "Same Aminal Acid Change",
                    "description": "Same AA change as established pathogenic variant?",
                    "type": "boolean"
                },
                "truncatingCauseDisease": {
                    "title": "Truncating Cause Disease",
                    "description": "Only truncating cause disease?",
                    "type": "boolean"
                }
            }
        },
        "inFrameIndel": {
            "title": "In-Frame Indel",
            "type": "object",
            "properties": {
                "regionRepetitive": {
                    "title": "Region Repetitive",
                    "type": "boolean"
                },
                "proteinLengthChanges": {
                    "title": "Protein Length Changes",
                    "description": "Protein length changes as a result of In-Frame deletion/insertion in a non-repeat region or stop-loss variant?",
                    "type": "boolean"
                },
                "noKnownFunction": "No Known Function",
                "description": "In-Frame deletion/insertion in a repetitive region without a known function?",
                "type": "boolean"
            }
        },
        "nonsenseOrFrameshiftOrSplice12": {
            "type": "object",
            "properties": {
                "totalExons": {
                    "title": "Total Exons",
                    "description": "Total number of exons",
                    "type": "number"
                },
                "affectedExons": {
                    "title": "Affected Exons",
                    "description": "List of affected exons",
                    "type": "array",
                    "items": {
                        "title": "Affected Exon",
                        "type": "number"
                    }
                },
                "lof": {
                    "title": "Known LOF",
                    "description": "Object containing LOF data",
                    "type": "object",
                    "properties": {
                        "prematureStopLocation": {
                            "type": "object",
                            "properties": {
                                "residue": {
                                    "title": "Residue",
                                    "type": "string"
                                },
                                "exon": {
                                    "title": "Exon",
                                    "type": "number"
                                },
                                "genomic": {
                                    "title": "Genomic",
                                    "type": "string"
                                },
                                "match55bpRule": {
                                    "title": "Match 55bp Rule",
                                    "description": "Greater than 55bp from penult exon",
                                    "type": "boolean"
                                }
                            }
                        },
                        "lofMachanisms": {
                            "type": "object",
                            "properties": {
                                "gene": {
                                    "title": "Gene",
                                    "type": "string",
                                    "linkTo": "gene"
                                },
                                "mechanisms": {
                                    "title": "LOF Mechanisms",
                                    "description": "List of LOF mechanisms related to the gene",
                                    "type": "array",
                                    "items": {
                                        "title": "Mechanism",
                                        "type": "object",
                                        "properties": {
                                            "disease": {
                                                "title": "Disease",
                                                "type": "string",
                                                "linkTo": "orphaPhenotype"
                                            },
                                            "knownMechanism": {
                                                "title": "Known Mechanism",
                                                "type": "boolean"
                                            },
                                            "sourceOfLOF": {
                                                "title": "Source of LOF Data",
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "silentOrOtherSplice": {
            "type": "object",
            "properties": {
                "splicePredictors": {
                    "title": "Splice Predictions",
                    "type": "string"
                },
                "averageChange": {
                    "title": "Average Change",
                    "description": "Average change at nearest splice site",
                    "type": "number"
                }
            }
        }
    },
    "columns": {
        "variant.represent": {
            "title": "Variant",
            "type": "string"
        },
        "molecular_consequence": {
            "title": "Molecular Consequence",
            "type": "string"
        }
    }
}

