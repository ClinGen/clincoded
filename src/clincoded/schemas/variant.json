
{
    "title": "Variant",
    "id": "/profiles/variant.json",
    "description": "Schema for storing and using variant data",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [],
    "identifyingProperties": ["uuid"],
    "additionalProperties": false,
    "mixinProperties": [
        { "$ref": "mixins.json#/schema_version" },
        { "$ref": "mixins.json#/uuid" },
        { "$ref": "mixins.json#/standard_status" },
        { "$ref": "mixins.json#/submitted" }
    ],
    "properties": {
        "schema_version": {
            "default": "3"
        },
        "clinvarVariantId": {
            "title": "Variant ID",
            "description": "ClinVar Variant ID",
            "type": "string",
            "default": ""
        },
        "carId": {
            "title": "CAR ID",
            "description": "Canonical Allele Registration ID from Baylor's Allele Registration",
            "type": "string",
            "default": ""
        },
        "clinvarVariantTitle": {
            "title": "ClinVar Variant Title",
            "type": "string",
            "default": ""
        },
        "clinVarRCVs": {
            "title": "ClinVar RCV Accession",
            "description": "List of ClinVar RCV ids associated to the variant",
            "type": "array",
            "default": [],
            "items": {
                "type": "string"
            }
        },
        "clinVarSCVs": {
            "title": "ClinVar SCV Accession",
            "description": "List of ClinVar SCV ids associated to the variant",
            "type": "array",
            "default": [],
            "items": {
                "type": "string"
            }
        },
        "dbSNPIds": {
            "title": "dbSNP ID",
            "description": "List of dbSNP ids related of the variant",
            "type": "array",
            "default": [],
            "items": {
                "type": "string"
            }
        },
        "hgvsNames": {
            "title": "HGVS Terms",
            "description": "Object containing HGVS terms. Separated to GRCh37, GRCh38 and others",
            "type": "object",
            "default": {}
        },
        "otherDescription": {
            "title": "Other Description",
            "description": "List of other descriptions",
            "type": "string",
            "default": ""
        }
    },
    "columns": {
        "uuid": {
            "title": "Variant",
            "type": "string"
        },
        "clinvarVariantId": {
            "title": "ClinVar Variation ID",
            "type": "string"
        },
        "clinvarVariantTitle": {
            "title": "ClinVar Prefered Name",
            "type": "string"
        },
        "carId": {
            "title": "CAR ID",
            "type": "string"
        },
        "otherDescription": {
            "title": "Other Description",
            "type": "string"
        }
    }
}
