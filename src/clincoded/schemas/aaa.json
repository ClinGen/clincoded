{
    "title": "Individual",
    "id": "/profiles/individual.json",
    "description": "Schema for storing and using individual data",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": ["individualid", "diagnosis", "method"],
    "identifyingProperties": ["uuid", "individualid"],
    "additionalProperties": false,
    "mixinProperties": [
        { "$ref": "mixins.json#/schema_version" },
        { "$ref": "mixins.json#/uuid" },
        { "$ref": "mixins.json#/shared_status" }
    ],
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "individualid": {
            "title": "IndividualID",
            "description": "Digital unique id for individual",
            "type": "string",
            "pattern": "^[0-9]{1,12}$",
            "uniqueKey": true
        },
        "diagnosis": {
            "title": "Diagnosis",
            "description": "Orpha Number to diagnosis",
            "type": "string"
        },
        "sex": {
            "title": "Sex",
            "description": "Sex",
            "type": "string",
            "default": ""
        },
        "method": {
            "title": "Method",
            "description": "Mothed id",
            "type": "string"
        }
    },
    "columns": {
        "individualid": {
            "title": "Individual",
            "type": "string"
        },
        "diagnosis": {
            "title": "Diagnosis",
            "type": "string"
        },
        "sex": {
            "title": "Sex",
            "type": "string"
        },
        "method": {
            "title": "Method",
            "type": "string"
        }
    }
}
